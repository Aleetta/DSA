SELECTION SORT
#include <stdio.h>
void readarray(int A[],int n){
    printf("Enter Elements!\n");
    for(int i=0;i<n;i++){
        scanf("%d",&A[i]);}
}

void selsort(int A[],int n){
    int i,j;
    for(i=0;i<=n-2;i++){
        for(j=i+1;j<=n-1;j++){
            if(A[i]>A[j]){
                int temp=A[i];
                A[i]=A[j];
                A[j]=temp;
            }
        }
    }
}

void printarray(int A[],int n){
    printf("Sorted Array: ");
    for(int i=0;i<n;i++){
        printf("%d ",A[i]);}
}

void main()
{int n;
    printf("Enter no. of elements!");
    scanf("%d",&n);
    int A[n];
    readarray(A,n);
    selsort(A,n);
    printarray(A,n);
}
